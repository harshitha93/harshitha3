<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.harshitha.talentacquisition" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Apply"></activity>
        <activity android:name=".Search"></activity>
        <activity android:name=".Forward"></activity>
        <activity android:name=".AdvanceSearch"></activity>
        <activity android:name=".Filters"></activity>
        <activity android:name=".JobRequest"></activity>
        <activity android:name=".Attach"></activity>
        <activity android:name=".UserProfile"></activity>
        <activity android:name=".Preferences"></activity>
    </application>

</manifest>
